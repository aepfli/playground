{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/js-flagsmith-provider/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/js-flagsmith-provider",
        "main": "libs/js-flagsmith-provider/src/index.ts",
        "tsConfig": "libs/js-flagsmith-provider/tsconfig.lib.json",
        "target": "node",
        "compiler": "tsc"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/js-flagsmith-provider/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/js-flagsmith-provider"],
      "options": {
        "jestConfig": "libs/js-flagsmith-provider/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
